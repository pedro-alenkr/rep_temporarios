@{
    ViewData["Title"] = "Login";
}

<div class="modal fade" id="modal-default">
    <div class="modal-dialog">
        <div class="modal-content">

            <h1>Login</h1>
            <div>
                <table>
                    <tr>
                        <td>
                            <asp:TextBox ID="username"></asp:TextBox>
                        </td>
                        <td>
                            <asp:TextBox ID="password"></asp:TextBox>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <asp:Button ID="submit"></asp:Button>
                        </td>
                    </tr>
                </table>
            </div>

            <form method="post" id="form">

                <div class="modal-body">
                   
                </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Close</button>
                    <button type="submit" onclick="Send(event, '/Home/InsertTeste')" class="btn btn-success">marcar como lido</button>
                </div>

            </form>

        </div>
    </div>
</div>